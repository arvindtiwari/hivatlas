<%= form_for ([@user,@message]), url: user_messages_path do |f| %>
  <p>
    <%= f.label :sender %><br>
    <%= f.text_field :sender %>

  </p>
 
  <p>
    <%= f.label :subject %><br>
    <%= f.text_field :subject %>

   </p>



   <p>
    <%= f.label :message %><br>
    <%= f.text_area :message %>

   </p>

   
   <%= f.submit %>
   <% end %>