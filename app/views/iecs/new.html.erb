<%= form_for ([@iec]),url:  user_iecs_path  do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>

  </p>


  <p>
    <%= f.label :body %><br>
    <%= f.text_area :body %>

  </p>



  <p>
    <%= f.label :language %><br>
    <%= f.text_field :language %>

  </p>



  <p>
    <%= f.label :location %><br>
    <%= f.text_field :location %>

  </p>

   <%= f.submit 'save' %>
   <% end %>